exports.getHomeViewState = getHomeViewState;

function getHomeViewState(state) {
  return {
    characters: mapCharactersToPresentables(state.characters),
    viewState: state.viewState
  };
}

function mapCharactersToPresentables(characters) {
  return characters.map(function (c) {
    return {
      name: c.name.toUpperCase()
    };
  });
}
